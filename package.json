    "styleguide:build": "styleguidist build",
    "styleguide:deploy": "gh-pages -d styleguide",
    "build": "babel src/index.js --out-file dist/index.js"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "ramda": "0.25.0"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.8",
    "gh-pages": "^1.1.0",
    "milligram": "^1.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-styleguidist": "^6.1.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0"
